{
  "name": "salesforce-data-cleaner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "cf:login": "npx wrangler login",
    "cf:build": "npm run build",
    "cf:deploy": "npx wrangler pages deploy dist --project-name=salesforce-data-cleaner",
    "cf:deploy:worker": "npx wrangler deploy",
    "cf:kv:create": "npx wrangler kv:namespace create \"DATA_STORE\"",
    "cf:kv:list": "npx wrangler kv:key list --binding=DATA_STORE",
    "cf:tail": "npx wrangler tail",
    "cf:dev": "npx wrangler pages dev dist --port 8788"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "wrangler": "^3.78.12"
  }
}